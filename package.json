{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "ENVFILE=.env.dev react-native run-android",
    "bundle:qa": "ENVFILE=.env.qa react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:uat": "ENVFILE=.env.uat react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android:prod": "cd android && ENVFILE=.env.prod ./gradlew assembleRelease",
    "keytool": "cd android/app && keytool -genkey -v -keystore my-release-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "ios": "rm -rf ~/Library/Developer/Xcode/DerivedData/template-*/Build/Intermediates.noindex/Pods.build/*/react-native-config.build && react-native run-ios --scheme 'DEV'",
    "ios:qa": "rm -rf ~/Library/Developer/Xcode/DerivedData/template-*/Build/Intermediates.noindex/Pods.build/*/react-native-config.build && react-native run-ios --scheme 'QA'",
    "ios:uat": "rm -rf ~/Library/Developer/Xcode/DerivedData/template-*/Build/Intermediates.noindex/Pods.build/*/react-native-config.build && react-native run-ios --scheme 'UAT'",
    "ios:prod": "rm -rf ~/Library/Developer/Xcode/DerivedData/template-*/Build/Intermediates.noindex/Pods.build/*/react-native-config.build && react-native run-ios",
    "lint": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "jest",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-firebase/app": "^18.4.0",
    "@react-native-firebase/messaging": "^18.4.0",
    "@react-navigation/bottom-tabs": "^7.0.0-alpha.3",
    "@react-navigation/native": "^7.0.0-alpha.3",
    "@react-navigation/stack": "^6.3.17",
    "appcenter": "^5.0.0",
    "appcenter-analytics": "^5.0.0",
    "appcenter-crashes": "^5.0.0",
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "i18next": "^23.5.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.4",
    "react-native-code-push": "^8.1.0",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-localize": "^3.0.2",
    "react-native-rename": "^3.2.13",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "12.1.1",
    "react-native-vector-icons": "^10.0.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.14",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "patch-package": "^8.0.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  }
}
